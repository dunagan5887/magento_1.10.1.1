<logging>
    <enterprise_banner translate="label">
        <label>Manage Banners</label>
        <expected_models>
            <enterprise_banner__banner/>
        </expected_models>
        <actions>
            <adminhtml_banner_edit>
                <action>view</action>
            </adminhtml_banner_edit>

            <adminhtml_banner_save>
                <action>save</action>
                <skip_on_back>
                    <adminhtml_banner_edit />
                </skip_on_back>
            </adminhtml_banner_save>

            <adminhtml_banner_delete>
                <action>delete</action>
            </adminhtml_banner_delete>

            <adminhtml_banner_massDelete>
                <action>massDelete</action>
            </adminhtml_banner_massDelete>
        </actions>
    </enterprise_banner>
</logging>
